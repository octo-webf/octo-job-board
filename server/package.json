{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:autoreload": "nodemon ./bin/www",
    "startmon": "nodemon ./bin/www",
    "clean": "yarn clean:dependencies && yarn clean:db",
    "clean:dependencies": "rm -rf node_modules",
    "clean:db": "rm src/db/data.development.sqlite",
    "configure": "./tools/setup/setup.sh",
    "lint": "./node_modules/.bin/eslint --ext .js src test",
    "lint:fix": "./node_modules/.bin/eslint --fix --ext .js src test",
    "test": "NODE_ENV=test nyc mocha --recursive",
    "test:watch": "NODE_ENV=test ./node_modules/mocha/bin/mocha --recursive --watch",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "db": "./node_modules/.bin/sequelize",
    "db:help": "./node_modules/.bin/sequelize help",
    "db:reset": "yarn clean:db && yarn db:migrate",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:rollback": "./node_modules/.bin/sequelize db:migrate:undo"
  },
  "dependencies": {
    "@slack/client": "^3.11.0",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "~3.1.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-http-to-https": "^1.1.3",
    "google-auth-library": "^0.10.0",
    "jade": "^1.11.0",
    "jsonwebtoken": "^7.4.2",
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "node-mailjet": "^3.2.1",
    "node-schedule": "^1.2.4",
    "pg": "^7.3.0",
    "request": "^2.81.0",
    "sequelize": "^4.4.4",
    "sequelize-cli": "^2.8.0",
    "serve-favicon": "~2.4.3",
    "sqlite3": "^3.1.9"
  },
  "devDependencies": {
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^2.13.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-recommended": "^1.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.5.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.1.0",
    "sinon": "^3.2.0",
    "sinon-chai": "^2.14.0",
    "supertest": "^3.0.0"
  }
}
